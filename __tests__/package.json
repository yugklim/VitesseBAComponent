{
  "name": "VitesseBA",
  "version": "0.0.1",
  "author": "Eugene Klimenko",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "rootDir": "../",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__/webpack.config.js"
    ],
    "transform": {
      ".*": "<rootDir>/__tests__/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "enzyme": "<rootDir>/tests/node_modules/enzyme",
      "sinon": "<rootDir>/tests/node_modules/sinon"
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "enzyme": "^2.7.1",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0"
  },
  "dependencies": {
    "karma": "^1.7.0",
    "lodash": "^4.17.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
}
